	Keywords = {Animals,Bird Diseases,Bird Diseases: virology,Birds,Cluster Analysis,Female,Genome, Viral,Louisiana,Mice,Mice, Inbred BALB C,Molecular Sequence Data,Mutation, Missense,Phylogeny,Point Mutation,RNA, Viral,RNA, Viral: genetics,Sequence Analysis, DNA,Virulence,West Nile Fever,West Nile Fever: pathology,West Nile Fever: virology,West Nile virus,West Nile virus: genetics,West Nile virus: isolation \& purification,West Nile virus: pathogenicity},
	Month = apr,
