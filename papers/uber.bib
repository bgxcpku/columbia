	Title = {{Use of the l1 norm for selection of sparse parameter sets that accurately predict drug response phenotype from viral genetic sequences.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1560612\&tool=pmcentrez\&rendertype=abstract},
